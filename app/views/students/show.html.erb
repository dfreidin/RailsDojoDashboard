<h1><%= @student.dojo.branch %>'s <%= @student.first_name %> <%= @student.last_name %></h1>
<p>Email: <%= @student.email %>
<h2>Cohort</h2>
<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        <% @student.dojo.students.where(created_at: (@student.created_at.beginning_of_day..@student.created_at.end_of_day)).where.not(id: @student.id).each do |s| %>
            <tr>
                <td><%= s.first_name %></td>
                <td><%= s.last_name %></td>
                <td><%= s.email %></td>
            </tr>
        <% end %>
    </tbody>
<table>
<p><a href="<%= dojo_url(@student.dojo.id) %>">Home</a> | <a href="<%= edit_dojo_student_url(@student.dojo.id, @student.id) %>">Edit</a></p>