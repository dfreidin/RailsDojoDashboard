<h1>New Stuent</h1>
<form action="<%= dojo_students_url %>" method="POST">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <p>First Name: <input type="text" name="student[first_name]"></p>
    <p>Last Name: <input type="text" name="student[last_name]"></p>
    <p>Email Address: <input type="text" name="student[email]"></p>
    <p>Dojo: <select name="student[dojo]">
        <% Dojo.all.each do |d| %>
            <option <% if d == @dojo %>selected<% end %>><%= d.branch %></option>
        <% end %>
    </select></p>
    <p><input type="submit" value="Create"></p>
</form>
<p><a href="/dojos">Home</a> | <a href="<%= dojo_url(@dojo.id) %>"><%= @dojo.branch %></a></p>